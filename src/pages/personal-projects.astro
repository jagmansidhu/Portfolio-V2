---
import Header from '../components/Header.astro';
import Project from '../components/Project.astro';
import Sidebar from '../components/Sidebar.astro';
const projects = [
  {
    githubLink: "https://github.com/jagmansidhu/DaRoomate",
    title: "DaRoomate",
    desc: [
      "Built a full stack Roomate management app to help landlord manage properties",
      "Developed in SpringBoot, React and published using AWS"
    ],
    tech: ["SpringBoot", "React", "Postgres", "AWS", "Docker"]
  },
  {
    githubLink: "https://github.com/jagmansidhu/DiagnosisHealthNLP",
    title: "MentalHealthDiagnosis",
    desc: [
      "Built a machine learning pipeline that automatically retrains on new labeled data to continuously refine diagnostic accuracy",
      "Fine-tuned a Transformers-based NLP model to identify key diagnostic indicators from text"
    ],
    tech: ["Python", "Pandas", "NumPy", "Transformers"]
  }
];
---

<!DOCTYPE html>
<html
>
  <Header title="Personal Projects | Jagman Sidhu" />
  <body>
    <div class="content">
        <Sidebar selectedItem="personal-projects" />
      <nav class="main">
        <div class="container">
          <div class="back-nav-container">
            <a href="/" class="back-nav-button w-inline-block"
              ><img src="/assets/back.svg" loading="lazy" alt="" class="back-nav-img" />
              <div>Back</div></a
            >
          </div>
          <div class="content-container">
            <h1>Personal Projects</h1>
            <section class="items-container">
                <div class="resouce-items">
                    {projects.map(project => (
                      <Project 
                        title={project.title} 
                        desc={project.desc}
                        tech={project.tech}
                        projectLink={project.projectLink}   
                        githubLink={project.githubLink}
                      />
                    ))}
                  </div>
            </section>
          </div>
        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body>
</html>
